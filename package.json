{
  "$schema": "https://raw.githubusercontent.com/SchemaStore/schemastore/refs/heads/master/src/schemas/json/package.json",
  "name": "sanctuary-dev",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "pnpm": {
    "overrides": {
      "sanctuary": "workspace:*",
      "sanctuary-constant": "workspace:*",
      "sanctuary-def": "workspace:*",
      "sanctuary-descending": "workspace:*",
      "sanctuary-either": "workspace:*",
      "sanctuary-identity": "workspace:*",
      "sanctuary-int": "workspace:*",
      "sanctuary-list": "workspace:*",
      "sanctuary-maybe": "workspace:*",
      "sanctuary-pair": "workspace:*",
      "sanctuary-scripts": "workspace:*",
      "sanctuary-set": "workspace:*",
      "sanctuary-show": "workspace:*",
      "sanctuary-style": "workspace:*",
      "sanctuary-type-classes": "workspace:*",
      "sanctuary-type-identifiers": "workspace:*",
      "sanctuary-types": "workspace:*",
      "sanctuary-useless": "workspace:*"
    }
  }
}
